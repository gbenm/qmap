{
  "name": "qmapjs",
  "packageManager": "yarn@3.2.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'src/**/*.test.ts'",
    "prebuild": "yarn lint && yarn test",
    "build": "tsc && yarn postbuild",
    "postbuild": "tscpaths -p tsconfig.json -s ./src -o ./dist"
  },
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/eslint": "^8.4.1",
    "@types/lodash": "^4.14.181",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "chai": "^4.3.6",
    "eslint": "^8.13.0",
    "mocha": "^9.2.2",
    "ts-node": "^10.7.0",
    "tscpaths": "^0.0.9",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "antlr4": "~4.9.3",
    "lodash": "^4.17.21"
  }
}
